{
  "name": "@panoptic-xyz/sdk",
  "version": "0.0.1-alpha.2",
  "type": "module",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "biome check .",
    "format": "biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "CI=True vitest",
    "size": "size-limit"
  },
  "peerDependencies": {
    "viem": ">=1"
  },
  "size-limit": [
    {
      "name": "panoptic-sdk (cjs)",
      "path": "./dist/index.cjs",
      "limit": "16 kB"
    },
    {
      "name": "panoptic-sdk",
      "path": "./dist/index.js",
      "limit": "8 kB",
      "import": "*"
    }
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@size-limit/preset-small-lib": "^9.0.0",
    "@uniswap/v3-core": "^1.0.1",
    "@viem/anvil": "^0.0.6",
    "@vitest/coverage-v8": "^0.34.6",
    "size-limit": "^9.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "viem": "^1.16.4",
    "vitest": "^0.34.6"
  },
  "dependencies": {
